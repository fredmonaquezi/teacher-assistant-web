# Frontend-safe Supabase configuration
# Use the project URL and the ANON/PUBLISHABLE key only.
VITE_SUPABASE_URL="https://your-project-ref.supabase.co"
VITE_SUPABASE_ANON_KEY="your-anon-or-publishable-key"

# Optional auth feature flags
VITE_ENABLE_GOOGLE_AUTH="false"
# Optional but recommended for auth email links.
# Set this to your public app URL in production so email links never point to localhost.
VITE_PUBLIC_APP_URL="https://yourdomain.com"
# Required only when Google auth is enabled
VITE_GOOGLE_CLIENT_ID="your-google-web-client-id.apps.googleusercontent.com"
